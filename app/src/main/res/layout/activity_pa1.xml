                            android:layout_height="wrap_content"
                            android:text="Impedancia"
                            android:textAppearance="?android:attr/textAppearanceLarge"/>

                        <EditText
                            android:id="@+id/Impedanciae"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="numberDecimal" />



                        <TextView
                            android:layout_gravity="fill"
                            android:gravity="left"
                            android:id="@+id/Frecuencia"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Frecuencia"
                            android:textAppearance="?android:attr/textAppearanceLarge"/>

                        <EditText
                            android:id="@+id/FrecuenciaE"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="numberDecimal" />


                        <TextView
                            android:layout_gravity="fill"
                            android:gravity="left"
                            android:id="@+id/Altitud"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Altitud"
                            android:textAppearance="?android:attr/textAppearanceLarge"/>

                        <EditText
                            android:id="@+id/AltitudE"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="numberDecimal" />


                        <TextView
                            android:layout_gravity="fill"
                            android:gravity="left"
                            android:id="@+id/Fecha"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Fecha de Fabricacion"
                            android:textAppearance="?android:attr/textAppearanceLarge"/>

                        <EditText
                            android:id="@+id/FechaE"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="numberDecimal" />


                        <TextView
                            android:layout_gravity="fill"
                            android:gravity="left"
                            android:id="@+id/Temp_Aceite"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Temperatura del Aceite"
                            android:textAppearance="?android:attr/textAppearanceLarge"/>

                        <EditText
                            android:id="@+id/Temp_AceiteE"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="numberDecimal" />



                        <TextView
                            android:layout_gravity="fill"
                            android:gravity="left"
                            android:id="@+id/Humedad"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="% de Humedad"
                            android:textAppearance="?android:attr/textAppearanceLarge"/>

                        <EditText
                            android:id="@+id/HumedadE"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="numberDecimal" />


                        <TextView
                            android:layout_gravity="fill"
                            android:gravity="left"
                            android:id="@+id/Lts_Aceite"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Lts. de Aceite"
                            android:textAppearance="?android:attr/textAppearanceLarge"/>

                        <EditText
                            android:id="@+id/Lts_AceiteE"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="numberDecimal" />


                        <TextView
                            android:layout_gravity="fill"
                            android:gravity="left"
                            android:id="@+id/Fases"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Fases"
                            android:textAppearance="?android:attr/textAppearanceLarge"/>

                        <EditText
                            android:id="@+id/FasesE"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="numberDecimal" />



                        <TextView
                            android:layout_gravity="fill"
                            android:gravity="left"
                            android:id="@+id/Peso"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Peso Total"
                            android:textAppearance="?android:attr/textAppearanceLarge"/>

                        <EditText
                            android:id="@+id/PesoE"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="numberDecimal" />


                        <TextView
                            android:layout_gravity="fill"
                            android:gravity="left"
                            android:id="@+id/Tap_Operacion"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Tap. de Operación"
                            android:textAppearance="?android:attr/textAppearanceLarge"/>

                        <EditText
                            android:id="@+id/Tap_OperacionE"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="numberDecimal" />


                        <Button
                            android:id="@+id/btnSig"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Siguiente"
                            android:layout_gravity="left"
                            android:onClick="onClick"
                            android:textAppearance="?android:attr/textAppearanceLarge"/>

                <com.jjoe64.graphview.GraphView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/graph" />

                </GridLayout>

        </HorizontalScrollView>

</ScrollView>

